{
	"name": "stranerd",
	"description": "Stranerd App",
	"version": "0.0.1",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"postinstall": "yarn jetify",
		"serve": "vue-cli-service serve",
		"preserve": "yarn bin:project:env",
		"build": "vue-cli-service build",
		"build-app": "vue-cli-service build --skip-plugins compression",
		"prebuild": "yarn bin:project:env",
		"prebuild-app": "yarn bin:project:env && rm -rf android/app/build",
		"postbuild-app": "cap sync",
		"lint": "vue-cli-service lint",
		"prepare": "husky install",
		"ionic:serve": "yarn serve",
		"prerun:android": "rm -rf android/app/build",
		"run:android": "ionic cap run android --livereload --external",
		"run:ios": "ionic cap run ios --livereload --external",
		"bin:project:setup": "node bin/project.js setup",
		"bin:project:version": "node bin/project.js version",
		"bin:project:env": "node bin/project env",
		"bin:icons": "capacitor-assets generate --ios --android",
		"android:debug:apk": "yarn build-app && cd android && ./gradlew assembleDebug && ./gradlew installDebug && cd ..",
		"android:release:apk": "yarn build-app && cd android && ./gradlew assembleRelease && cd .. && yarn android:sign:apk",
		"android:release:bundle": "yarn build-app && cd android && ./gradlew bundleRelease && cd .. && yarn android:sign:bundle",
		"android:sign:bundle": "apksigner sign --ks ./android/stranerd.keystore --ks-key-alias kevin@stranerd.com --min-sdk-version 22 ./android/app/build/outputs/bundle/release/app-release.aab",
		"android:sign:apk": "zipalign 4 ./android/app/build/outputs/apk/release/app-release-unsigned.apk ./android/app/build/outputs/apk/release/app-release.apk && apksigner sign --ks ./android/stranerd.keystore --ks-key-alias kevin@stranerd.com ./android/app/build/outputs/apk/release/app-release.apk"
	},
	"dependencies": {
		"@awesome-cordova-plugins/core": "^5.45.0",
		"@awesome-cordova-plugins/file-opener": "^5.45.0",
		"@capacitor/android": "^4.2.0",
		"@capacitor/app": "4.0.1",
		"@capacitor/clipboard": "^4.0.1",
		"@capacitor/core": "4.2.0",
		"@capacitor/dialog": "^4.0.1",
		"@capacitor/filesystem": "^4.1.1",
		"@capacitor/haptics": "4.0.1",
		"@capacitor/ios": "^4.2.0",
		"@capacitor/keyboard": "4.0.1",
		"@capacitor/preferences": "^4.0.1",
		"@capacitor/push-notifications": "^4.1.0",
		"@capacitor/share": "^4.0.1",
		"@capacitor/splash-screen": "^4.0.1",
		"@capacitor/status-bar": "4.0.1",
		"@capacitor/toast": "^4.0.1",
		"@codetrix-studio/capacitor-google-auth": "^3.2.0",
		"@ionic/pwa-elements": "^3.1.1",
		"@ionic/vue": "^6.2.7",
		"@ionic/vue-router": "^6.2.7",
		"@stranerd/validate": "^3.4.0",
		"axios": "^0.27.2",
		"capacitor-blob-writer": "1.1.3",
		"cordova-plugin-file-opener2": "^3.0.5",
		"core-js": "^3.25.1",
		"katex": "^0.16.2",
		"socket.io-client": "^4.5.2",
		"sweetalert2": "^11.4.33",
		"vue": "^3.2.39",
		"vue-pdf-embed": "^1.1.4",
		"vue-router": "^4.1.5",
		"vue-waypoint": "^4.2.5",
		"vue3-editor": "^0.1.1",
		"workbox-background-sync": "^6.5.4",
		"workbox-expiration": "^6.5.4",
		"workbox-precaching": "^6.5.4",
		"workbox-routing": "^6.5.4",
		"workbox-strategies": "^6.5.4",
		"workbox-webpack-plugin": "^6.5.4",
		"workbox-window": "^6.5.4"
	},
	"devDependencies": {
		"@capacitor/assets": "^1.0.1",
		"@capacitor/cli": "4.2.0",
		"@commitlint/cli": "^17.1.2",
		"@commitlint/config-conventional": "^17.1.0",
		"@ionic/cli": "^6.20.1",
		"@trapezedev/project": "^5.0.3",
		"@typescript-eslint/eslint-plugin": "^5.37.0",
		"@typescript-eslint/parser": "^5.37.0",
		"@vue/cli-plugin-babel": "^5.0.8",
		"@vue/cli-plugin-eslint": "^5.0.8",
		"@vue/cli-plugin-pwa": "^5.0.8",
		"@vue/cli-plugin-router": "^5.0.8",
		"@vue/cli-plugin-typescript": "^5.0.8",
		"@vue/cli-service": "^5.0.8",
		"@vue/compiler-sfc": "^3.2.39",
		"@vue/eslint-config-typescript": "^11.0.1",
		"autoprefixer": "^10.4.11",
		"eslint": "^8.23.1",
		"eslint-plugin-vue": "^9.5.1",
		"husky": "^8.0.1",
		"jetifier": "^2.0.0",
		"postcss": "^8.4.16",
		"sass": "^1.54.9",
		"sass-loader": "^13.0.2",
		"tailwindcss": "^3.1.8",
		"typescript": "~4.8.3",
		"unplugin-vue-components": "^0.22.7",
		"vue-cli-plugin-compression": "~2.0.0"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	}
}
