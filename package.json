{
	"name": "stranerd",
	"description": "Stranerd App",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"serve": "vue-cli-service serve",
		"preserve": "yarn bin:copyenvs",
		"build": "vue-cli-service build",
		"prebuild": "yarn bin:copyenvs && yarn bin:config:set",
		"postbuild": "yarn cap sync",
		"test:unit": "vue-cli-service test:unit",
		"test:e2e": "vue-cli-service test:e2e",
		"lint": "vue-cli-service lint",
		"prepare": "husky install",
		"run:android": "ionic cap run android -l --external --port 8080",
		"run:ios": "ionic cap run ios -l --external --port 8080",
		"bin:copyenvs": "node bin/copy-env-app",
		"bin:config:set": "node bin/generate-app-configs.js set && rm android/app/build -rf",
		"bin:config:reset": "node bin/generate-app-configs.js reset",
		"android:debug:apk": "yarn build && cd android && ./gradlew assembleDebug && ./gradlew installDebug && cd .. && yarn bin:config:reset",
		"android:release:apk": "yarn build && cd android && ./gradlew assembleRelease && cd .. && yarn android:sign:apk && yarn bin:config:reset",
		"android:release:bundle": "yarn build && cd android && ./gradlew bundleRelease && cd .. && yarn android:sign:bundle && yarn bin:config:reset",
		"android:sign:bundle": "apksigner sign --ks ./android/stranerd.keystore --ks-key-alias kevin@stranerd.com --min-sdk-version 22 ./android/app/build/outputs/bundle/release/app-release.aab",
		"android:sign:apk": "zipalign 4 ./android/app/build/outputs/apk/release/app-release-unsigned.apk ./android/app/build/outputs/apk/release/app-release.apk && apksigner sign --ks ./android/stranerd.keystore --ks-key-alias kevin@stranerd.com ./android/app/build/outputs/apk/release/app-release.apk"
	},
	"dependencies": {
		"@capacitor/android": "^3.4.0",
		"@capacitor/app": "1.1.0",
		"@capacitor/clipboard": "^1.0.7",
		"@capacitor/core": "3.4.1",
		"@capacitor/dialog": "^1.0.7",
		"@capacitor/filesystem": "^1.1.0",
		"@capacitor/haptics": "1.1.4",
		"@capacitor/ios": "^3.4.1",
		"@capacitor/keyboard": "1.2.2",
		"@capacitor/push-notifications": "^1.0.9",
		"@capacitor/share": "^1.1.1",
		"@capacitor/splash-screen": "^1.2.2",
		"@capacitor/status-bar": "1.0.8",
		"@capacitor/storage": "^1.2.4",
		"@capacitor/toast": "^1.0.8",
		"@codetrix-studio/capacitor-google-auth": "^3.1.0",
		"@ionic/pwa-elements": "^3.1.1",
		"@ionic/vue": "6.0.0",
		"@ionic/vue-router": "6.0.0",
		"@stranerd/validate": "^2.0.0",
		"axios": "^0.25.0",
		"core-js": "^3.21.0",
		"get-video-id": "^3.5.3",
		"katex": "^0.15.2",
		"pdf-parse": "^1.1.1",
		"pdfvuer": "^2.0.1",
		"socket.io-client": "^4.4.1",
		"sweetalert2": "^11.4.0",
		"vue": "^3.2.30",
		"vue-router": "^4.0.12",
		"vue3-editor": "^0.1.0-alpha.3",
		"vue3-youtube": "^0.1.9",
		"youtube-video-exists": "^0.1.2"
	},
	"devDependencies": {
		"@capacitor/cli": "3.4.1",
		"@commitlint/cli": "^16.1.0",
		"@commitlint/config-conventional": "^16.0.0",
		"@ionic/cli": "^6.18.1",
		"@ionic/lab": "^3.2.10",
		"@types/vue2-editor": "^2.6.1",
		"@typescript-eslint/eslint-plugin": "^2.33.0",
		"@typescript-eslint/parser": "^4.31.0",
		"@vue/cli-plugin-babel": "~4.5.15",
		"@vue/cli-plugin-eslint": "~4.5.15",
		"@vue/cli-plugin-router": "~4.5.15",
		"@vue/cli-plugin-typescript": "~4.5.15",
		"@vue/cli-service": "~4.5.15",
		"@vue/compiler-sfc": "^3.2.30",
		"@vue/eslint-config-typescript": "^5.0.2",
		"autoprefixer": "^9.8.7",
		"eslint": "^6.7.2",
		"eslint-plugin-vue": "^7.0.0-0",
		"husky": "^7.0.4",
		"node-sass": "^6.0.1",
		"postcss": "^7.0.38",
		"sass-loader": "^10.1.1",
		"tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17",
		"typescript": "^4.5.5"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	}
}
